#include <stdio.h>
#include <stdlib.h>
#include <stdbool.h>
#include<locale.h>

int main()
{
    setlocale(LC_ALL,"Portuguese");
    int tabuada = 0;
    printf("\nDigite a tabuada que vocÃª deseja visualizar: \n\n> ");
    scanf("%i", &tabuada);

    for(int x = 1; x<=10; ++x){
        printf("%i x %i = %i\n", x, tabuada, x * tabuada);
    }

    return 0;
}
